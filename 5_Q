SELECT PNO FROM WORKS_ON W, EMPLOYEE E WHERE E.SSN=W.SSN AND E.LNAME='SCOTT' UNION SELECT PNO
FROM PROJECT P, DEPARTMENT D,EMPLOYEE E WHERE P.DNO=D.DNO AND D.MGRSSN=E.SSN AND E.LNAME='SCOTT';

SELECT E.SSN,E.SALARY*1.1 AS RAISE FROM EMPLOYEE E,PROJECT P,WORKS_ON W WHERE E.SSN=W.SSN AND
P.PNO=W.PNO AND P.PNAME='IOT';

SELECT D.DNAME,MAX(SALARY),MIN(SALARY),AVG(SALARY) FROM EMPLOYEE E, DEPARTMENT D WHERE E.DNO=D.DNO AND D.DNAME='ACCOUNTS' GROUP BY E.DNO;

SELECT FNAME,LNAME FROM EMPLOYEE E WHERE NOT EXISTS (SELECT * FROM WORKS_ON B WHERE (B.PNO IN
(SELECT PNO FROM PROJECT WHERE DNO=5) AND NOT EXISTS(SELECT * FROM WORKS_ON C WHERE C.SSN=E.SSN AND C.PNO=B.PNO)));

SELECT D.DNO, COUNT(*) FROM DEPARTMENT D, EMPLOYEE E WHERE D.DNO=E.DNO AND E.SALARY>600000 AND D.DNO IN (SELECT E1.DNO FROM EMPLOYEE E1 GROUP BY E1.DNO HAVING COUNT(*)>5) GROUP BY D.DNO;
